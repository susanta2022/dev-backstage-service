pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('Test'){
            steps {
                 echo 'Test World'
            }
        }
        
        stage('Deployed'){
            steps {
                 echo 'Deployed World'
            }
        }
        
    }
}
